{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Streamlit (logic app)",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": ["run", "${workspaceFolder}/streamlit_logic_app.py", "--server.port", "8501"],
      "justMyCode": true,
      "env": {}
    }
    ,
    {
      "name": "Run Flask (logic app)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/flask_logic_app.py",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
